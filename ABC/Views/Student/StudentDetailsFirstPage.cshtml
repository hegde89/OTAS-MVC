@model IEnumerable<ABC.Models.GetTeacherDetailsByUSN_Result>

@{
    ViewBag.Title = "StudentDetailsFirstPage";
}

<h2>Student Details Page</h2>
<br />
@if(Model!=null)
{
<table style="width:100%;">
    <tr>
        <th><b>Subject Code</b></th>
        <th><b>Subject Name</b></th>
        <th><b>Teacher name</b></th>
        <th><b>Elective</b></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.SubCode)</td>
       
            <td>@Html.DisplayFor(m => item.SubName)</td>
        
            <td>@Html.DisplayFor(m => item.TeacherName)</td>
        

            @if (item.Elective == true)
            {
                <td>True</td>
            }
            else
            {
                <td>False</td>
            }

        </tr>
    }
</table>
}
<br/>
@using (Html.BeginForm("StudentReviews","Student", FormMethod.Get))
{
    <input type="submit" value="Proceed" class="btn btn-default" />
}